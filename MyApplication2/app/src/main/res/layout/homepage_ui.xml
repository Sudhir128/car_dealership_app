<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/menubarimage"
        android:layout_width="46dp"
        android:layout_height="39dp"
        app:srcCompat="@drawable/bars"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageView
        android:id="@+id/userimage"
        android:layout_width="46dp"
        android:layout_height="39dp"
        app:srcCompat="@drawable/user_solid"
        tools:layout_editor_absoluteX="349dp"
        tools:layout_editor_absoluteY="30dp" />

    <EditText
        android:id="@+id/nametext"
        android:layout_width="243dp"
        android:layout_height="54dp"
        android:background="@drawable/red_round_rectangle"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="  Search..."
        android:textAlignment="viewStart"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="148dp" />

    <ImageView
        android:id="@+id/searchimage"
        android:layout_width="38dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/search_line_icon"
        tools:layout_editor_absoluteX="287dp"
        tools:layout_editor_absoluteY="158dp" />

    <ImageView
        android:id="@+id/addcarimage"
        android:layout_width="99dp"
        android:layout_height="81dp"
        android:background="@drawable/red_round_rectangle"
        android:tag="newcar"
        app:srcCompat="@drawable/add_car_icon"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="272dp" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Car"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/addcarimage"
        tools:layout_editor_absoluteX="104dp" />

    <ImageView
        android:id="@+id/buycarimage"
        android:layout_width="99dp"
        android:layout_height="81dp"
        android:background="@drawable/red_round_rectangle"
        app:srcCompat="@drawable/access_hand_key_icon"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="272dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy Car"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/buycarimage"
        tools:layout_editor_absoluteX="233dp" />

    <ImageView
        android:id="@+id/sellcarimage"
        android:layout_width="99dp"
        android:layout_height="81dp"
        android:background="@drawable/red_round_rectangle"
        app:srcCompat="@drawable/car_vehicle_sell_icon"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sell Car"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/sellcarimage"
        tools:layout_editor_absoluteX="104dp" />

    <ImageView
        android:id="@+id/serviceimage"
        android:layout_width="99dp"
        android:layout_height="81dp"
        android:background="@drawable/red_round_rectangle"
        app:srcCompat="@drawable/garage_icon"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/serviceimage"
        tools:layout_editor_absoluteX="233dp" />

</androidx.constraintlayout.widget.ConstraintLayout>